{
    "name": "premium-blog-root",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "server",
        "client",
        "admin"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev -w server\" \"npm run dev -w client\" \"npm run dev -w admin\"",
        "build": "npm run build -w client && npm run build -w admin",
        "install:all": "npm install",
        "preview:local": "concurrently \"npm run start -w server\" \"npm run preview -w client\" \"npm run preview -w admin\""
    },
    "dependencies": {
        "express": "^5.2.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "sequelize": "^6.37.7",
        "pg": "^8.17.1",
        "pg-hstore": "^2.3.4",
        "jsonwebtoken": "^9.0.3",
        "bcryptjs": "^3.0.3",
        "multer": "^2.0.2",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "vite": "^7.2.4",
        "@vitejs/plugin-react": "^4.3.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    }
}